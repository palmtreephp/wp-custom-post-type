{
    "name": "palmtree/wp-custom-post-type",
    "description": "WordPress Custom Post Types",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "palmtree/argparser": "^2.0.0",
        "palmtree/nameconverter": "^2.0.0"
    },
    "require-dev": {
        "roots/wordpress": "^5.6.0",
        "palmtree/php-cs-fixer-config": "^2.0"
    },
    "authors": [
        {
            "name": "Andy Palmer",
            "email": "andy@andypalmer.me"
        }
    ],
    "autoload": {
        "psr-4": {
            "Palmtree\\WordPress\\CustomPostType\\": "src"
        }
    },
    "extra": {
        "wordpress-install-dir": "vendor/roots/wordpress"
    },
    "scripts": {
        "fix": "vendor/bin/php-cs-fixer fix --diff"
    },
    "config": {
        "allow-plugins": {
            "roots/wordpress-core-installer": true
        }
    }
}
